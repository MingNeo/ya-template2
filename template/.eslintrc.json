{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
    "standard"
  ],
  "plugins": [
    "standard",
    "flowtype-errors",
    "vue"
  ],
  "globals": {
    "$": true,
    "jQuery": true,
    "_": true,
    "R": true,
    "Vue": true
  },
  "rules": {
    "flowtype-errors/show-errors": 2
  }
}